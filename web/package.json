{
  "name": "diekeditora",
  "private": true,
  "workspaces": [
    "packages/studio",
    "packages/app"
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.14.8",
    "@types/node": "^15.12.3",
    "@types/react": "^17.0.11",
    "@types/react-relay": "^11.0.2",
    "@types/relay-runtime": "^11.0.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-relay": "^11.0.2",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "relay-compiler": "^11.0.2",
    "relay-compiler-language-typescript": "^14.0.0",
    "stylelint": "^13.13.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.4",
    "watchman": "^1.0.0"
  },
  "scripts": {
    "lint": "eslint './**/*.ts' './**/*.tsx' './**/*.js' --fix",
    "dev": "run-p app:dev studio:dev relay:watch",
    "app:build": "yarn workspace @diekeditora/app run build",
    "app:start": "yarn workspace @diekeditora/app run start",
    "app:storybook": "yarn workspace @diekeditora/app run storybook",
    "app:test": "yarn workspace @diekeditora/app run cypress:run",
    "app:dev": "run-p graphql:dev app:dev-start",
    "app:dev-start": "yarn workspace @diekeditora/app run dev",
    "relay": "yarn workspace @diekeditora/graphql run relay",
    "studio:dev": "run-p graphql:dev studio:dev-start",
    "studio:dev-start": "yarn workspace @diekeditora/studio run dev",
    "studio:build": "yarn workspace @diekeditora/studio run build",
    "studio:relay": "yarn workspace @diekeditora/studio run relay"
  },
  "dependencies": {
    "@chakra-ui/theme": "^1.10.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@firebase/app": "^0.6.29",
    "@firebase/database": "^0.10.9",
    "depd": "^2.0.0",
    "encoding": "^0.1.13",
    "follow-redirects": "^1.14.1",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.1",
    "keygrip": "^1.1.0",
    "next-transpile-modules": "^8.0.0",
    "react-is": "^17.0.2",
    "react-relay": "^11.0.2",
    "relay-runtime": "^11.0.2"
  }
