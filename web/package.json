{
  "name": "diekeditora",
  "private": true,
  "workspaces": ["packages/store","packages/studio", "packages/app"],
  "devDependencies": {
    "@types/node": "^15.12.3",
    "@types/react": "^17.0.11",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.24.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.4"
  },
  "scripts": {
    "lint": "eslint './**/*.ts' './**/*.tsx' './**/*.js' --fix",
    "dev": "run-p app:dev studio:dev",
    "app:build": "yarn workspace @diekeditora/app run build",
    "app:start": "yarn workspace @diekeditora/app run start",
    "app:test": "yarn workspace @diekeditora/app run cypress:run",
    "app:dev": "run-p store:dev app:dev-start",
    "app:dev-start": "yarn workspace @diekeditora/app run dev",
    "studio:dev": "run-p store:dev studio:dev-start",
    "studio:dev-start": "yarn workspace @diekeditora/studio run dev",
    "studio:build": "yarn workspace @diekeditora/studio run build",
    "store:dev": "yarn workspace @diekeditora/store run dev"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.17",
    "next": "^11.0.0",
    "next-transpile-modules": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  }
}
