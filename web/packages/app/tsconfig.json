{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@diekeditora/app/*": ["./*"],
      "@diekeditora/app": ["./"],
      "@diekeditora/ui/*": ["../ui/*"],
      "@diekeditora/ui": ["../ui"],
      "@diekeditora/store/*": ["../store/*"],
      "@diekeditora/store": ["../store"]
    },
    "target": "es5"
  },
  "include": ["**/*.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["cypress", "node_modules"]
}
