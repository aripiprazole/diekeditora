{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@diekeditora/app/*": ["./src/*"],
      "@diekeditora/app": ["./src"],
      "@diekeditora/store/*": ["../store/src/*"],
      "@diekeditora/store": ["../store/src"]
    },
    "target": "es5"
  },
  "include": ["**/*.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["cypress", "node_modules"]
}
