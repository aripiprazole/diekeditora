{
  "name": "@diekeditora/store",
  "version": "0.0.1",
  "description": "Diek editora store",
  "keywords": [
    "store",
    "diekeditora"
  ],
  "author": "Lorenzo Guimar√£es <lorenzooguimaraes@gmail.com>",
  "homepage": "https://github.com/diekeditora/diekeditora",
  "license": "ISC",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diekeditora/diekeditora.git"
  },
  "scripts": {
    "dev": "run-p watch:decl watch:build",
    "watch:decl": "tsc -w",
    "watch:build": "babel src --watch --extensions \".ts\" --out-dir dist",
    "compile": "babel src --extensions \".ts\" --out-dir dist",
    "gen-decl": "tsc",
    "build": "yarn run gen-decl && yarn run compile"
  },
  "bugs": {
    "url": "https://github.com/diekeditora/diekeditora/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/node": "^16.0.0",
    "babel-helper-regex": "^6.26.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0"
  }
}
